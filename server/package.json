  "type": "module",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/mysql": "^2.15.12",
    "@types/node": "^14.0.5",
    "concurrently": "^5.2.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/compression": "^1.7.0",
